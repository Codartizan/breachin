+compute_box_volume(BoxNode *b) {
+    unsigned char rl, rh, gl, gh, bl, bh;
+    if (b->volume >= 0) {
+        return b->volume;
+    }
+    if (!b->head[0]) {
+        b->volume = 0;
+    } else {
+        rh = b->head[0]->p.c.r;
+        rl = b->tail[0]->p.c.r;
+        gh = b->head[1]->p.c.g;
+        gl = b->tail[1]->p.c.g;
+        bh = b->head[2]->p.c.b;
+        bl = b->tail[2]->p.c.b;
+        b->volume = (rh - rl + 1) * (gh - gl + 1) * (bh - bl + 1);
+    }
+    return b->volume;
 }

