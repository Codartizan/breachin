+#define BINOP(name, op, type)                          \
+    void name(Imaging out, Imaging im1, Imaging im2) { \
+        int x, y;                                      \
+        for (y = 0; y < out->ysize; y++) {             \
+            type *p0 = (type *)out->image[y];          \
+            type *p1 = (type *)im1->image[y];          \
+            type *p2 = (type *)im2->image[y];          \
+            for (x = 0; x < out->xsize; x++) {         \
+                *p0 = op(type, *p1, *p2);              \
+                p0++;                                  \
+                p1++;                                  \
+                p2++;                                  \
+            }                                          \
+        }                                              \
+    }

