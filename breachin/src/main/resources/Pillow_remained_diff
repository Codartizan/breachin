+void
+normalize_angles(float *al, float *ar) {
+    if (*ar - *al >= 360) {
+        *al = 0;
+        *ar = 360;
+    } else {
+        *al = fmod(*al < 0 ? 360 - (fmod(-*al, 360)) : *al, 360);
+        *ar = *al + fmod(*ar < *al ? 360 - fmod(*al - *ar, 360) : *ar - *al, 360);
+    }
 }

