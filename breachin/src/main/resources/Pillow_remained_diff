 // Resulting angles will satisfy 0 <= al < 360, al <= ar <= al + 360
-void normalize_angles(float* al, float* ar) {
-  if (*ar - *al >= 360) {
-    *al = 0;
-    *ar = 360;
-  } else {
-    *al = fmod(*al < 0 ? 360 - (fmod(-*al, 360)) : *al, 360);
