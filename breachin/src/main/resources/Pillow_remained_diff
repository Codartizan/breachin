+free_box_tree(BoxNode *n) {
+    PixelList *p, *pp;
+    if (n->l) {
+        free_box_tree(n->l);
+    }
+    if (n->r) {
+        free_box_tree(n->r);
+    }
+    for (p = n->head[0]; p; p = pp) {
+        pp = p->next[0];
+        free(p);
+    }
+    free(n);
