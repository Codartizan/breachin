+    unsigned int i;
+
+    /* malloc check ok, calloc for overflow */
+    paletteArray = calloc(paletteLength, sizeof(Pixel));
+    if (!paletteArray) {
+        return NULL;
+    }
+
+    for (i = 0; i < paletteLength; i++) {
+        avg_color_from_color_bucket(&palette[i], &paletteArray[i]);
