+static PyObject *
+font_setvarname(FontObject *self, PyObject *args) {
+    int error;
+
+    int instance_index;
+    if (!PyArg_ParseTuple(args, "i", &instance_index)) {
+        return NULL;
+    }
+
+    error = FT_Set_Named_Instance(self->face, instance_index);
+    if (error) {
+        return geterror(error);
+    }
+
+    Py_INCREF(Py_None);
+    return Py_None;
+}
