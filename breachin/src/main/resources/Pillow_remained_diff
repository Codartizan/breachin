+void
+clip_ellipse_free(clip_ellipse_state *s) {
+    while (s->head != NULL) {
+        event_list *t = s->head;
+        s->head = s->head->next;
+        free(t);
+    }
+}
+
+int8_t
+clip_ellipse_next(
+    clip_ellipse_state *s, int32_t *ret_x0, int32_t *ret_y, int32_t *ret_x1) {
+    int32_t x0, y, x1;
+    while (s->head == NULL && ellipse_next(&s->st, &x0, &y, &x1) >= 0) {
+        if (clip_tree_do_clip(s->root, x0, y, x1, &s->head) < 0) {
+            return -2;
+        }
+        s->y = y;
+    }
+    if (s->head != NULL) {
+        *ret_y = s->y;
+        event_list *t = s->head;
+        s->head = s->head->next;
+        *ret_x0 = t->x;
+        free(t);
+        t = s->head;
+        assert(t != NULL);
+        s->head = s->head->next;
+        *ret_x1 = t->x;
+        free(t);
+        return 0;
+    }
+    return -1;
 }

