+hash_to_list(const HashTable *h, const Pixel pixel, const uint32_t count, void *u) {
+    PixelHashData *d = (PixelHashData *)hashtable_get_user_data(h);
+    PixelList **pl = (PixelList **)u;
+    PixelList *p;
+    int i;
+    Pixel q;
+
