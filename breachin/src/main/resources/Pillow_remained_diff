+    if (!t) {
+        return 0;
+    }
+    t->key = key;
+    t->next = *n;
+    *n = t;
+    if (newFunc) {
+        newFunc(h, t->key, &(t->value));
+    } else {
+        free(t);
+        return 0;
+    }
+    h->count++;
+    _hashtable_resize(h);
+    return 1;
