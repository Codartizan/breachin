+                            pb = abs(a - c);
+                            pc = abs(a + b - 2 * c);
+
+                            /* pick predictor with the shortest distance */
+                            state->buffer[i] += (pa <= pb && pa <= pc) ? a
+                                                : (pb <= pc)           ? b
+                                                                       : c;
